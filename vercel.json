{
  "version": 2,
  "functions": {
    "server/index.js": {
      "runtime": "@vercel/node"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/approve/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/verify-email/(.*)",
      "dest": "/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/client/build/index.html"
    }
  ]
} 